/* pages/cate.wxss */

.cate{
   padding-top: 46px;
}

.cate .title{
  font-weight: bold;
  font-size: 40rpx;
  text-align: center;
  padding-bottom: 20rpx;
}

.cate .name{
  font-size: 36rpx;
  margin: 10rpx 10rpx;
  background-color: #3385ff;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
}
.items{
  text-align: center;
  margin: 30rpx 0;  
}
.bg-bul{
  background-color: blue;
}
.item-name{
  justify-content: center;
  flex-wrap:wrap;
  display: flex;
  flex-direction: row;
}

.cate-title{
  color: #fff;
  position: fixed;
  top: 0;
  padding: 20rpx;
  display: inline-block;
  text-align: center;
  font-size:40rpx;
  width: 100%;
}
.brand{
  color: #409EFF;
}
.success{
  color: #67C23A;
}

.bg-brand{
  background-color: #409EFF;
}
.bg-success{
  background-color: #E6A23C;
}